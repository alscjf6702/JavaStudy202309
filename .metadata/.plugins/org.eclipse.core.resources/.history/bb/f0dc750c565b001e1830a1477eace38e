package ex01;

import java.io.File;

public class Aasdsda {

	public static void main(String[] args) {
		
		File file = new File("C:/data/test12.txt");
	
		try {
			boolean ss  = file.createNewFile();
			
			if(ss) {
				System.out.println("파일 생성 성공");
			}else{
				System.out.println("파일 생성 실패");
			}
		} catch (Exception e) {
			System.out.println(e);
		}
		
	}
	
}

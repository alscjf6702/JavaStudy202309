package ex07;



class Box<T> {

	private T ob;

	public Box(T ob) {this.ob=ob;}
	
	public T getOb() {
		return ob;
	}

	public void setOb(T ob) {
		this.ob = ob;
	}

	@Override
	public String toString() {
		return ob.toString();
	}

}

class Unboxer{
	public static <T> T openBox(Box<T> box) {
		return box.get();
	}
	
	public static void peekBox(Box<?> box) {
		System.out.println();
	}
}

public class WildCardUnBox {
	
	
	
}

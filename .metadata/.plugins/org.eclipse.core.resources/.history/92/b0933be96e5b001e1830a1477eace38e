package ex02_2;

class Apple {
	public String toString() {
		return "I am an Apple";
	}
}

class Orange {
	public String toString() {
		return "I am an Orange";
	}
}

//제네릭 클래스
class Box<T> {
	private T obj;

	void setObj(T obj) {
		this.obj = obj;
	}

	T getObj() {
		return obj;
	}
}

public class Generices1 {

	public static void main(String[] args) {
		
	Box aBox = new Box();
//	aBox.setObj(new Apple());
	System.out.println(aBox.getObj());
	
	
	}
}

package ex10;

import java.util.ArrayList;

public class Exam01 {
	public static void main(String[] args) {

		ArrayList<String> a = new ArrayList<String>();
		
		//기본 16칸
		
		int[] b = new int[10];
		
		a.add("Kor");
		a.add("10");
		
		
		ArrayList g = new ArrayList();
		
		g.add("gg");
		g.add(10);
		g.add(10.4);
		System.out.println(a);
		System.out.println(g);
		
	
	}
}

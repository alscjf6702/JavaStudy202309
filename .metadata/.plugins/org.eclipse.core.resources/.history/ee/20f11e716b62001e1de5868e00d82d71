package ex06;

@FunctionalInterface
interface Arr {
	int sumArr(int[] arr);
}



public class Lambda9 {

	public static void main(String[] args) {

		
		int sumArr(int[] arr) {
			int sum  = 0 ;
			for(int i = arr) {
				sum+=i;
			}
			return sum;
		}
		
//		
//		Arr ar = (int[] arr)-> {int sum = 0; for(int i : arr) sum+=i; 
//		return sum;
//		};
		int[] arr55 = {1,2,4,6,7,8,4,2,6,6,72};
		
		System.out.println(ar.sumArr(arr55));
		

	}

}

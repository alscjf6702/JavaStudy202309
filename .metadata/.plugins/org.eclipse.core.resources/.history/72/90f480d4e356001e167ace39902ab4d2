
public class Exam02 {

	
	public static void main(String[] args) {
	
		System.out.println(" 1부터 30까지의 정수에서 짝수만 한 줄에 3개씩 출력하라");
		ex05();
		
	}
	
	
	public static void ex05() {
		for(int i = 2; i<=30 ; i=i+2) {
			if(i%4 == 0) {
				System.out.println("\n ");
			}
			System.out.print(i + " ");
		}
		
	}
}

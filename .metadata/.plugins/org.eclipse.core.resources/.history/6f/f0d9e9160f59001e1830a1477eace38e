package ex03;

import java.io.File;
import java.io.IOException;

public class ExceptionEx02 {

	public static void main(String[] args) {
		
		File file = new File("Member.txt");
		
		if(!file.exists()) {
			try {
				file.createNewFile();
			} catch (IOException e) {
			
				e.printStackTrace();
			}
		}
		
	}
	
}

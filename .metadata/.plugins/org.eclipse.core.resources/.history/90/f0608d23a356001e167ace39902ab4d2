package ex04;

public class Lotto {

	public static void main(String[] args) {
		
		int[] lottoNum = new int[6];
		
		for(int i = 0 ; i < lottoNum.length;i++) {
			int num = (int)(Math.random()*45)+1;
			lottoNum[i] = num;
			System.out.print(lottoNum[i]+" ");
		}
		for(int i = 0 ; i<lottoNum.length ;i++) {
			if(lottoNum[i]==lottoNum[i+1]) {
				int num = (int)(Math.random()*45)+1;
				lottoNum[i]=num;
			}
		}
		
		
	}
}

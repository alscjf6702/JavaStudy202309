package ex09;

public class Exam01 {

	public static void main(String[] args) {
		int[] numA = new int[10];
		int count =0;
		int k = 0;
		for (int i = 0; i < numA.length; i++) {
			numA[i] = (int) (Math.random() * 10);		
		}
		
		for(int i = 0; i<numA.length; i++) {
			System.out.print(numA[i]+ " ");
		}

		for(int i=0; i<numA.length; i++) {
			if(numA[i] == 0) {
				int zero = i;
				System.out.println("\n0의 개수 : " + zero);
			}
		}
		
		
	}

}
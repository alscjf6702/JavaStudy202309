package ex06;

import java.util.function.Consumer;

public class Lambda6_1 {

	public static void main(String[] args) {

		Consumer<String> con = new Consumer<String>() {
			
			@Override
			public void accept(String name) {
				System.out.println(name);
			}
			
		};
		
		con.accept("조조");
		
	}

}

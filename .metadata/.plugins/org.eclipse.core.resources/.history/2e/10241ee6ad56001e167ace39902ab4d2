package ex04;

import java.util.Iterator;

public class Exam02 {

	public static void main(String[] args) {
		
		int[] numA = new int[10];
		
		for(int i = 0 ; i<numA.length; i++) {
			numA[i] = i+1;
		}
		
		for(int i = 0 ; i<numA.length; i++) {
			System.out.println(numA[i]+ " ");
		}
		System.out.println("원 데이터 : " );
		for(int i = 0; i<100; i++) {
			int tmp = (int)(Math.random()*10); //0~9
			tmp = numA[0];
			numA[0] = numA[tmp];
			numA[tmp] = tmp;
		}
		
		System.out.println("썩은 데이터 : " );
		for(int i = 0; i<numA.length; i++) {
			System.out.println(numA[i]+" ");
		}
	}
	
}

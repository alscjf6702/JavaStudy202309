package ex09_Inher;

public class PhoneExam01 {

	public static void main(String[] args) {
		
		Phone p = new Phone();
		p.bell();
		p.sendVoice("안녕 친구야");
		p.hangUp();
		
		System.out.println("========");
		
		SmartPhone sp = new SmartPhone();
		sp.bell();
		sp.sendVoice("그래 친구야");
		sp.hangUp();
	}
}
